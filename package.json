{
  "name": "ice-react",
  "version": "1.0.0",
  "description": "react demo workplace",
  "main": "app.js",
  "scripts": {
    "start": "npm-run-all --sequential build start-prod",
    "dev": "npm-run-all --sequential build-server:dev start-dev",
    "build": "npm-run-all clean --parallel build-all:prod",
    "build-server:dev": "better-npm-run build-dev-server",
    "build-all:prod": "better-npm-run build-prod",
    "start-dev": "better-npm-run nodemon-dev",
    "start-prod": "better-npm-run node-prod",
    "clean": "rimraf dist",
    "pm2-start": "pm2 start process.json",
    "pm2-restart": "pm2 restart process.json"
  },
  "betterScripts": {
    "build-dev-server": {
      "command": "webpack --colors --config ./webpack/webpack.config.dev.server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "nodemon-dev": {
      "command": "nodemon server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "node-prod": {
      "command": "node server.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build-prod": {
      "command": "webpack --colors --config ./webpack/webpack.config.prod.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/icezeros/ice-react.git"
  },
  "keywords": [
    "react",
    "redux",
    "material",
    "es6/7"
  ],
  "author": "icezeros",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/icezeros/ice-react/issues"
  },
  "homepage": "https://github.com/icezeros/ice-react#readme",
  "dependencies": {
    "axios": "^0.14.0",
    "bootstrap": "^3.3.6",
    "eslint-plugin-react": "^5.2.2",
    "lodash": "^4.15.0",
    "material-design-icons": "^3.0.1",
    "material-ui": "^0.15.4",
    "moment": "^2.15.0",
    "react": "^15.2.1",
    "react-addons-css-transition-group": "^15.2.1",
    "react-addons-test-utils": "^15.2.1",
    "react-bootstrap": "^0.29.3",
    "react-cookie": "^0.4.5",
    "react-dom": "^15.2.1",
    "react-linked-input": "^1.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "react-s-alert": "^1.1.4",
    "react-tap-event-plugin": "^1.0.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.10",
    "redux": "^3.6.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-form": "^6.0.0-rc.3",
    "redux-immutablejs": "0.0.8",
    "redux-logger": "^2.6.1",
    "redux-router": "^1.0.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.9.2",
    "better-npm-run": "0.0.9",
    "classnames": "^2.2.5",
    "concurrently": "^2.2.0",
    "css-loader": "^0.23.1",
    "ejs": "^2.5.2",
    "es6-promise": "^3.2.1",
    "eslint": "^3.0.1",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-react": "^5.2.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.6.3",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "image-webpack-loader": "^2.0.0",
    "immutable": "^3.8.1",
    "jackblog-sass": "^1.0.6",
    "json-loader": "^0.5.4",
    "node-libs-browser": "^1.0.0",
    "nodemon": "^1.9.2",
    "npm-run-all": "^2.3.0",
    "pm2": "^2.0.12",
    "rimraf": "^2.5.3",
    "serve-favicon": "^2.3.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.1"
  }
}
